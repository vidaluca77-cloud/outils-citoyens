    "faits": {
      "type": "string"
    },
    "etablissement": {
      "type": "string"
    },
    "parents_contacts": {
      "type": "string"
    }
  },
  "required": [
    "niveau",
    "type",
    "faits",
    "etablissement"
  ],
  "additionalProperties": false
}